  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Account API",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "PORT": "5002"
      },
      "program": "${workspaceFolder}/AccountApi/bin/www"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Authen API",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "PORT": "5001"
      },
      "program": "${workspaceFolder}/AuthenApi/bin/www"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Resource API",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "PORT": "5003"
      },
      "program": "${workspaceFolder}/ResourceApi/bin/www"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Web API",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "PORT": "4000"
      },
      "program": "${workspaceFolder}/WebApi/bin/www"
    }
  ]
}